{
  "expo": {
    "name": "Pravado",
    "slug": "pravado-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pravado.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos for your profile.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to select profile pictures."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pravado.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "pravado-mobile-client"
      },
      "apiUrl": process.env.API_URL || "https://api.pravado.com",
      "supabaseUrl": process.env.SUPABASE_URL || "https://your-project.supabase.co",
      "supabaseAnonKey": process.env.SUPABASE_ANON_KEY || ""
    },
    "scheme": "pravado",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/pravado-app"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
