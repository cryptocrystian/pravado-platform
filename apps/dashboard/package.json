{
  "name": "@pravado/dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "pnpm --filter @pravado/types build && pnpm --filter @pravado/validators build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "pages:build": "pnpm run prebuild && pnpm dlx @cloudflare/next-on-pages",
    "pages:dev": "pnpm dlx @cloudflare/next-on-pages --watch",
    "pages:deploy": "pnpm pages:build && wrangler pages deploy .vercel/output/static"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "@mui/x-date-pickers": "^6.19.0",
    "@pravado/design-system": "workspace:*",
    "@pravado/types": "workspace:*",
    "@pravado/utils": "workspace:*",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/react-query": "^5.17.19",
    "axios": "^1.6.7",
    "date-fns": "^2.30.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.309.0",
    "next": "14.2.33",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.21.3",
    "recharts": "^2.10.4",
    "swagger-ui-react": "^5.11.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@cloudflare/next-on-pages": "^1.13.16",
    "@cloudflare/workers-types": "^4.20240117.0",
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.11.5",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.17",
    "axe-core": "^4.11.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "wrangler": "^3.24.0"
  }
}
